{
  "name": "Big Tech Poetry Slam",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tts",
    "webRequest"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "companies-hostnames.json"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "action": {}
}